 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not(in = a, out = result1);
    Not(in = b, out = result2);
    Nand(a = result1, b = result2, out = out);
}